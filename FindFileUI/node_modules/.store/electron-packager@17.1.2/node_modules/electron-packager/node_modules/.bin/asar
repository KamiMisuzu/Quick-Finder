#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Users/Administrator/Desktop/EveryFind/FindFileUI/node_modules/.store/@electron+asar@3.2.10/node_modules/@electron"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Users/Administrator/Desktop/EveryFind/FindFileUI/node_modules/.store/@electron+asar@3.2.10/node_modules/@electron"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@electron+asar@3.2.10/node_modules/@electron/asar/bin/asar.js" "$@"
else
  exec node  "$basedir/../../../../../@electron+asar@3.2.10/node_modules/@electron/asar/bin/asar.js" "$@"
fi
